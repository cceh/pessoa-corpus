    <text>
        <body>
            <div>
                <head><hi rend="underline">Raphael Baldaya:</hi></head>
                <list>
                    <item>
                        <label>I.</label> The Foundations of Astrology.<lb/>
                        <list>
                            <item>
                                <label>(a)</label> The elements (data.) </item>
                            <item>
                                <label>(b)</label> The <choice>
                                    <seg n="2">
                                        <add place="above">nativity</add>
                                    </seg>
                                    <seg n="1">horoscope</seg>
                                </choice> in general. </item>
                            <item>
                                <label>(c)</label> The progressed <choice>
                                    <abbr>h<am>.</am><hi rend="superscript">pe</hi></abbr>
                                    <expan>h<ex>orosco</ex>pe</expan>
                                </choice> in general. </item>
                        </list>
                    </item>
                    <item>
                        <label>II.</label>
                        <del rend="overstrike">The</del> Mundane Astrology <lb/>
                        <list>
                            <item>
                                <label>(a)</label> Generalities. </item>
                            <item>
                                <label>(b)</label> National horoscopes. </item>
                            <item>
                                <label>(c)</label> The horoscope of a <seg type="anchor">nation <add
                                        place="below">(Portugal)</add></seg>
                            </item>
                        </list>
                    </item>
                </list>
                <metamark rend="line" function="distinct"/>
                <ab>The Doctrine of Transits.</ab>
                <metamark rend="line" function="distinct"/>
                <ab>10/- per <choice>
                        <abbr>reg<am>.</am></abbr>
                        <expan>reg<ex>ular</ex></expan>
                    </choice> post to <choice>
                        <abbr>G</abbr>
                        <expan>G<ex>reat</ex></expan>
                    </choice> Britain</ab>
                <metamark rend="line" function="distinct"/>
                <ab>A Rationalist <seg type="certainty" cert="low" rend="cross below" resp="FP"
                        >Classic</seg></ab>
                <ab rend="indent">The <choice>
                        <abbr>Mat</abbr>
                        <expan>Mat<ex>hematical</ex></expan>
                    </choice>
                    <choice>
                        <abbr>Prin</abbr>
                        <expan>Prin<ex>ciples</ex></expan>
                    </choice> of <rs type="name" key="P24"><choice>
                            <abbr>Jo</abbr>
                            <expan>Jo<ex>sé</ex></expan>
                        </choice> Anastacio da Cunha</rs></ab>
                <metamark rend="line" function="distinct"/>
                <ab>100 Sonnets of <rs type="name" key="P15">Camoens</rs>.</ab>
                <metamark rend="line" function="distinct"/>
                <ab><rs type="name" key="P18">Anthero de Quental</rs>: Sonnets</ab>
                <metamark rend="line" function="end"/>
